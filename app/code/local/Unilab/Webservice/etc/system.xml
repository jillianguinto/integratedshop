<?xml version="1.0" ?>
<config>
    <sections>
        <webservice translate="label" module="webservice">
            <label>Webservice Settings</label>
            <tab>ulah</tab>
            <sort_order>500</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <tab>RRAExtmod</tab>
            <groups>
				<sitesettings translate="label">
                    <label>Website Settings</label>
                    <sort_order>370</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
						<websiteurl>
                            <label>Website URL</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </websiteurl>
						
						<loginurl>
                            <label>Login URL</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>11</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </loginurl>
						
						<logouturl>
                            <label>Logout URL</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>12</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </logouturl>

                        <accturl>
                            <label>My Account URL</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>13</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </accturl>
                        
                        <signupurl>
                            <label>Signup URL</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>14</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </signupurl>
						
                        <siteenabled>
                            <label>Enabled</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>20</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </siteenabled>
                        <sitegateway>
                            <label>Gateway</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <depends>
                                <siteenabled>1</siteenabled>
                            </depends> 
                        </sitegateway>
                        <sitetoken>
                            <label>Token</label>
                            <frontend_type>textarea</frontend_type>
                            <sort_order>40</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                             <depends>
                                <siteenabled>1</siteenabled>
                            </depends>
                        </sitetoken>
                    </fields>
                </sitesettings>

				<netsuiteregsettings translate="label">
                    <label>NetSuite Settings</label>
                    <sort_order>380</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                     	<netsuiteurl>
                            <label>Website URL</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </netsuiteurl>	
                        <netsuiteregenabled>
                            <label>Enabled</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>20</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </netsuiteregenabled>                       
                        <netsuitereggateway>
                            <label>Gateway</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>Note: Always add "/" at the end of address. Ex.: http://unilab.com.ph:8080/api/</comment>
                            <depends>
                                <netsuiteregenabled>1</netsuiteregenabled>
                            </depends> 
                        </netsuitereggateway>
                        <netsuiteregtoken>
                            <label>Token</label>
                            <frontend_type>textarea</frontend_type> 
                            <sort_order>40</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                             <depends>
                                <netsuiteregenabled>1</netsuiteregenabled>
                            </depends>
                        </netsuiteregtoken>
                    </fields>
                </netsuiteregsettings>
            </groups>
        </webservice>
    </sections>
</config>
