		$(".grand-child-appendLink").click(function(){
			$(this).siblings('.grand-grand-child-menu').slideToggle('slow');
			if($(this).attr('class') == 'grand-child-appendLink rm-append-active'){
				$( this ).removeClass( "rm-append-active" );
				$( this ).addClass( "rm-append-inactive" );
			}
			else
			{
				$( this ).addClass( "rm-append-active" );
				$( this ).removeClass( "rm-append-inactive" );
			}
		});

		$(window).resize(function() {
		  if ($(window).width() > 1348) {
			$('#mobile-nav').fadeOut();
		  }
		});
	});
} ) ( jQuery );
</script>